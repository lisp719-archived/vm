[user]
  name = lisp719
  email = test@example.com
[push]
  default = simple
[core]
  editor = nano
  excludesfile = /home/vagrant/.gitignore
[alias]
  a = add
  ame = commit -v --amend
  b = branch
  bk = branch -f bk
  c = checkout
  cl = clean
  co = commit
  cu = checkout -b current
  d = diff
  dt = difftool
  f = fetch
  g = gui
  k = !gitk
  l = log --graph --decorate --oneline
  m = merge
  mt = mergetool
  p = pull
  ps = push
  psc = push origin HEAD
  r = rebase
  ra = rebase --abort
  rc = rebase --continue
  rs = reset
  s = status -b -s
  sh = show
  st = stash
  sync = reset --hard
  t = tag
[gui]
  gcwarning = false
