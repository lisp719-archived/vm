export PATH=$HOME/bin:$PATH
export PATH=$HOME/bin/cli:$PATH

export PS1='\u:\w$ '
export TERM=screen-256color
export TERMINAL=lxterminal

cdf() { cd $(find . -maxdepth 3 -type d | grep -v "\/\." | fzf); }
killf() { kill -9 `ps aux | fzf | awk '{print $2}'`; }

r() {
    if [ -z "$RANGER_LEVEL" ]
    then
        ranger
    else
        exit
    fi
}
